{
  "version": 2,
  "builds": [
    {
      "src": "api/send-email.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "RESEND_API_KEY": "@resend_api_key"
  }
}
